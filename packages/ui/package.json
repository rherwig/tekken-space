        "@types/node": "20.14.10",
        "@types/react": "19.0.8",
        "@types/react-dom": "19.0.3",
        "eslint": "9.19.0",
        "next": "15.1.6",
        "postcss": "8.5.1",
        "sass": "1.83.4",
        "storybook": "8.6.11",
